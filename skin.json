{
	"name": "P2 wiki",
	"author": [
		"[http://automattic.com/ Automattic]",
		"[https://www.mediawiki.org/wiki/User:Dantman Daniel Friesen]"
	],
	"descriptionmsg": "p2wiki-desc",
	"namemsg": "skinname-p2wiki",
	"url": "https://www.mediawiki.org/wiki/Skin:P2wiki",
	"type": "skin",
	"ValidSkinNames": {
		"p2wiki": {
			"class": "SkinTemplate",
			"args": [
				{
					"name": "p2wiki",
					"bodyOnly": true,
					"template": "P2wikiTemplate",
					"styles": [ "skins.p2wiki" ]
				}
			]
		}
	},
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"AutoloadClasses": {
		"P2wikiTemplate": "P2wiki.skin.php"
	},
	"MessagesDirs": {
		"SkinP2wiki": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "p2wiki"
	},
	"ResourceModules": {
		"skins.p2wiki": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"styles": {
				"style/screen.css": {
					"media": "screen"
				}
			}
		}
	},
	"manifest_version": 1
}
